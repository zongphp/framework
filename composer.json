{
  "name": "zongphp/zongphp",
  "description": "zongphp framework",
  "type": "project",
  "keywords": [
    "zongphp",
    "framework"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-soap": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "files": [
      "src/cache/helper.php",
	  "src/cli/helper.php",
	  "src/collection/helper.php",
	  "src/config/helper.php",
	  "src/controller/helper.php",
	  "src/crypt/helper.php",
	  "src/db/helper.php",
	  "src/error/helper.php",
	  "src/exception/helper.php",
	  "src/framework/helper.php",
	  "src/loader/helper.php",
	  "src/model/helper.php",
	  "src/request/helper.php",
	  "src/response/helper.php",
	  "src/route/helper.php",
	  "src/view/helper.php"
    ],
    "psr-4": {
      "zongphp\\framework\\": "src"
    }
  }
}
